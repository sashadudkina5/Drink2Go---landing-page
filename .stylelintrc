{
  "extends": "stylelint-config-htmlacademy",
  "rules": {
    "declaration-no-important": true,
    "indentation": [2, {"ignore": ["inside-parens"]}],
    "at-rule-no-unknown": null,
    "property-no-vendor-prefix": [true, {
      "ignoreProperties": ["appearance", "-moz-appearance"]
    }]
  }
}
